{
  "headers": [
    {
      "source": "/USA-LP/(.*)\\.html", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate" 
        }
      ]
    },
    {
      "source": "/USA-LP/(.*)\\.js", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable" 
        }
      ]
    },
    {
      "source": "/USA-LP/(.*)\\.css", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable" 
        }
      ]
    },
    {
      "source": "/USA-LP/(.*)\\.(png|jpg|jpeg|gif|webp|avif|svg)", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable" 
        }
      ]
    }
  ]
} 